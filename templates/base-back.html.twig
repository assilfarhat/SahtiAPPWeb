<script>
    $( document).ready(function() {
        var currentRequest = null;
        $("#searchsearch").keyup(function(e){
            var value = $(this).val();
            if(currentRequest != null) {
                currentRequest.abort();
            }
            currentRequest = $.ajax({

                url : "{{ path('search') }}",
                type : 'GET',
                data: {
                    'searchValue' : value
                },

                success : function(retour)
                {
                    $('#tableclient').html(retour);

                },
            });
            return false;
        });
    });
</script>
