{% for utils in donneeN %}
    <tr >

        <td>{{ utils.prenom }}</td>
        <td>{{ utils.nom }}</td>
        <td>{{ utils.adresse }}</td>
        <td>{{ utils.bio }}</td>
        <td>{{ utils.certification }}</td>

        <td>
            {% if utils.IsBlocked == false %}
                <span class="badge bg-label-success me-1">Active</span>

            {% else %}
                <span class="badge bg-label-warning me-1">Blocked</span>
            {% endif %}
        </td>
        <td>
            <a href="{{ path ('deletenutri',{'id' :utils.id}) }}" class="btn btn-danger" onclick="return confirm('Are you sure you want to delete this client?')"/> Delete</a>
            <a href="{{ path ('blocknutri',{'id' :utils.id}) }}" class="btn btn-warning" onclick="return confirm('Are you sure you want to block this client?')"/> Block</a>
            <a href="{{ path ('unblocknutri',{'id' :utils.id}) }}" class="btn btn-outline-warning" onclick="return confirm('Are you sure you want to unblock this client?')"/> Unblock</a>

        </td>
    </tr>
{% else %}
    <tr>
        <td colspan="5">no records found</td>
    </tr>
{% endfor %}